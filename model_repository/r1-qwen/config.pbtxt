backend: "vllm"
max_batch_size: 64

input [
  {
    name: "text_input"
    data_type: TYPE_STRING
    dims: [ -1 ]
  }
]

output [
  {
    name: "text_output"
    data_type: TYPE_STRING
    dims: [ -1 ]
  }
]

parameters [
  {
    key: "tensor_parallel_size"
    value: { string_value: "1" }  # Number of GPUs
  }
]

parameters: {
  key: "vllm:model_type"
  value: { string_value: "qwen" }
}